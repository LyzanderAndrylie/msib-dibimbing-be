  "description": "Dibimbing.id's Case Study Project in the MSIB 7 Program",
  "main": "./dist/index.js",
  "private": true,
  "scripts": {
    "build": "tspc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "format": "prettier ./src --write",
    "lint": "eslint ./src",
    "db:generate": "drizzle-kit generate",
